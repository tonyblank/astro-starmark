{
  "name": "starmark-integration",
  "version": "0.0.1",
  "description": "Astro StarMark feedback integration plugin",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "astro",
    "feedback",
    "starlight",
    "plugin",
    "linear",
    "auth0"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:component": "vitest run",
    "test:integration": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx,.astro",
    "lint:fix": "eslint src --ext .ts,.tsx,.astro --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,astro,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,astro,json,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "astro": "^5.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@astrojs/starlight": "^0.34.3",
    "@eslint/js": "^9.28.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^20.17.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^9.5.0",
    "eslint-plugin-astro": "^1.2.0",
    "globals": "^16.2.0",
    "happy-dom": "^17.6.3",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "astro": "^5.0.0"
  }
}