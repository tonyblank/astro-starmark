{
  "name": "starmark.dev",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "test": "echo 'No unit tests for starmark.dev - use test:e2e for E2E tests'",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx,.astro,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.astro,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,astro,js,jsx,json,md,mdx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,astro,js,jsx,json,md,mdx}\"",
    "type-check": "astro check",
    "clean": "rm -rf dist .astro"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.5.3",
    "@astrojs/starlight": "^0.34.3",
    "astro": "^5.8.2",
    "starmark-integration": "workspace:*"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.3",
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.45.0",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.5.0",
    "eslint-plugin-astro": "^1.2.0",
    "globals": "^16.2.0",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  }
}